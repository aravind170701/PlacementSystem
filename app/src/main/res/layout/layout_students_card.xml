<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/card_viewstudents"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:layout_marginLeft="16dp"
    android:layout_marginTop="12dp"
    android:layout_marginRight="16dp"
    android:clickable="true"
    card_view:cardBackgroundColor="#00FFFF"
    card_view:cardCornerRadius="12dp"
    card_view:contentPadding="10dp"
    tools:context=".fragments.admin.RecyclerViewAdapterSTUDENT">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/studentnameview"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="52dp"
            android:layout_marginBottom="24dp"
            android:fontFamily="sans-serif-black"
            android:gravity="top"
            android:textColor="#FFFFFF"
            android:text="Student Name: "
            android:textSize="16dp"
            card_view:layout_constraintBottom_toTopOf="@+id/studentbranchview"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/studentname"
            android:layout_width="85dp"
            android:layout_height="20dp"
            android:layout_marginStart="72dp"
            android:textColor="#FFFFFF"
            android:fontFamily="sans-serif-black"
            android:gravity="end|center_vertical"
            android:textSize="16dp"
            android:textAlignment="textStart"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.993"
            card_view:layout_constraintStart_toEndOf="@+id/studentnameview"
            card_view:layout_constraintTop_toBottomOf="@+id/studentid"
            card_view:layout_constraintVertical_bias="0.169"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/studentidview"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:textColor="#FFFFFF"
            android:layout_marginTop="5dp"
            android:layout_marginStart="10dp"
            android:fontFamily="sans-serif-black"
            android:text="Student ID: "
            android:textSize="16dp"
            card_view:layout_constraintBottom_toTopOf="@+id/studentnameview"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.058"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.2" />

        <TextView
            android:id="@+id/studentid"
            android:layout_width="85dp"
            android:textColor="#FFFFFF"
            android:layout_height="20dp"
            android:fontFamily="sans-serif-black"
            android:gravity="end|center_vertical"
            android:textAlignment="textStart"
            android:textSize="16dp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.982"
            card_view:layout_constraintStart_toEndOf="@+id/studentidview"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.062"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/studentbranchview"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="92dp"
            android:textColor="#FFFFFF"
            android:layout_marginBottom="24dp"
            android:fontFamily="sans-serif-black"
            android:text="Student Branch: "
            android:textSize="16dp"
            card_view:layout_constraintBottom_toTopOf="@+id/studentpercentageview"
            card_view:layout_constraintEnd_toStartOf="@+id/studentbranch"
            card_view:layout_constraintHorizontal_bias="0.0"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/studentbranch"
            android:layout_width="85dp"
            android:layout_height="20dp"
            android:layout_marginTop="20dp"
            android:textAlignment="textStart"
            android:textColor="#FFFFFF"
            android:fontFamily="sans-serif-black"
            android:gravity="end|center_vertical"
            android:textSize="16dp"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="1.0"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/studentname"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/studentpercentageview"
            android:layout_width="match_parent"
            android:textColor="#FFFFFF"
            android:layout_height="20dp"
            android:layout_marginStart="10dp"
            android:fontFamily="sans-serif-black"
            android:text="Student Percentage: "
            android:textSize="16dp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/studentpercentage"
            card_view:layout_constraintHorizontal_bias="0.142"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.85" />

        <TextView
            android:id="@+id/studentpercentage"
            android:layout_width="85dp"
            android:layout_height="20dp"
            android:layout_marginTop="20dp"
            android:textColor="#FFFFFF"
            android:fontFamily="sans-serif-black"
            android:gravity="end|center_vertical"
            android:textAlignment="textStart"
            android:textSize="16dp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="1.0"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/studentbranch"
            card_view:layout_constraintVertical_bias="0.111"
            tools:ignore="RtlCompat" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>



